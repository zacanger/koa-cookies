    "@types/node": "20.4.4",
    "koa": "2.14.2",
    "npm-run-all": "4.1.5",
    "sortpack": "2.3.4",
    "supertest": "6.3.3",
    "tap-spec": "5.0.0",
    "tape": "5.6.6",
    "ts-node": "10.9.1",
